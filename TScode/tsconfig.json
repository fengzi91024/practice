{
    "extends":"./tsconfig.base.json",
    "compilerOptions": {
        "outDir": "./dist",
        "target": "ESNext",
        "noEmitOnError": true, //有类型错误时，不进行编译
        "noImplicitAny": true, //是否对函数中隐式的any报错
        "strictPropertyInitialization": true, //是否初始化对象
        "strictNullChecks": true //严格的空值检查
    }
}